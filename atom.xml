  <entry>
    <title>方言及区别</title>
    <link href="http://wiki.zhouweitong.site/wiki/%E5%A4%9A%E5%9B%BD%E8%AF%AD/%E6%97%A5%E8%AF%AD/%E6%96%B9%E8%A8%80%E5%8F%8A%E5%8C%BA%E5%88%AB/"/>
    <id>http://wiki.zhouweitong.site/wiki/%E5%A4%9A%E5%9B%BD%E8%AF%AD/%E6%97%A5%E8%AF%AD/%E6%96%B9%E8%A8%80%E5%8F%8A%E5%8C%BA%E5%88%AB/</id>
